#! /usr/bin/env python

from inrimage import *
import numpy as np 
import sys

argc = len(sys.argv)

if argc == 1 or argc > 3:
    print('Usage: ...')
    exit(-1)
if argc == 3:
    outfile = sys.argv[2]
else:
    outfile = '>'

A = np.load(sys.argv[1])
A = np.float32(list(A.values())[0])
if len(A.shape) == 2:
    n,m = A.shape
    I = InrImage('float32',m,n)
    I.create(outfile)
    I.write(A)
    I.close()
else:
    print('shape not supported')
